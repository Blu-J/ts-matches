{
  "name": "ts-matches",
  "version": "1.1.0",
  "description": "We want to bring in some pattern matching into the typescript land. We want to be able to type guard a whole bunch of conditions and get the type back.",
  "main": "lib/matches.js",
  "types": "lib/matches.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "author": "Justin Miller",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "fast-check": "^1.5.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier --write \"src\\**\\*.(t|j)s(|x)\"",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/src/.*.spec.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "fp-ts": "^1.8.0"
  }
}
